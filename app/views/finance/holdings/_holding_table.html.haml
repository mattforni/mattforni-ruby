%table
    %tr
        %th
            Symbol
        %th
            Purchase Date
        %th
            Quantity
        %th
            Purchase Price
        %th
            Commission Price
    - holdings.each do |holding|
        %tr
            %td
                = holding.symbol
            %td
                = date_display holding.purchase_date
            %td.number-cell
                = decimal_display holding.quantity
            %td.number-cell
                = currency_display holding.purchase_price
            %td.number-cell
                = currency_display holding.commission_price

